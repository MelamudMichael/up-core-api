<settings>
  <servers>
    <server>
      <id>ossrh</id>
      <username>${env.ORG_OSSRH_USERNAME}</username>
      <password>${env.ORG_OSSRH_PASSWORD}</password>
    </server>
  </servers>

  <profiles>
    <profile>
      <id>ossrh</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <gpg.executable>gpg2</gpg.executable>
        <gpg.keyname>${env.ORG_GPG_PRIVATE_KEY}</gpg.keyname>
        <gpg.passphrase>${env.ORG_GPG_PASSPHRASE}</gpg.passphrase>
      </properties>
    </profile>
  </profiles>
  
</settings>
